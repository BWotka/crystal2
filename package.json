{
  "name": "crystal",
  "displayName": "Crystal",
  "description": "Updatet S.T.O.N.E Project, sends code to OneNote",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:crystal.sendPageToOneNote"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "crystal.sendPageToOneNote",
        "title": "Send Page to OneNote"
      },
      {
        "command": "crystal.sendSelectionToOneNote",
        "title": "Send Selection to OneNote"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack && copy-files",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "copy-files": "cp ./src/client.js ./dist && cp ./src/client.css ./dist"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/highlightjs": "^9.12.2",
    "@types/markdown-it": "^12.2.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/opn": "^5.5.0",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "express": "^4.18.1",
    "highlight.js": "^11.6.0",
    "markdown-it": "^13.0.1",
    "ncp": "^2.0.0",
    "open": "^8.4.0",
    "opn": "^6.0.0"
  }
}
